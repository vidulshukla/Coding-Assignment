#include<stdio.h>
#include<conio.h>
#include<string.h>
void main()
{
	char str[100],temp;
	int i,j;
	printf("Enter the string \n ");
	gets(str);
	i=0;
	j=strlen(str)-1;

	while(i<j)
	{
		temp = str[i];
		str[i] = str[j];
		str[j] = temp;
		i++;
		j--;
	}
	printf(" The reversed string is %s", str);
	getch();
}
